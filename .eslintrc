{
  "ecmaFeatures": { modules: true },
  "parser": "babel-eslint",
  "rules": {
    "curly": 0,
    "indent": [
      2,
      2
    ],
    "no-console": 0,
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "never"
    ],
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": false
  }
}
