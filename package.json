    "start": "node_modules/.bin/babel-node src/server.js",
    "test": "npm run lint-tests && npm run cover"
  },
  "devDependencies": {
    "babel-eslint": "4.1.6",
    "chai": "3.4.1",
    "chai-as-promised": "5.2.0",
    "coffee-coverage": "0.7.0",
    "coffee-script": "1.10.0",
    "coffeelint": "1.14.2",
    "eslint": "1.10.3",
    "isparta": "4.0.0",
    "mocha": "2.3.4",
    "mocha-sinon": "1.1.4",
    "sinon": "1.17.2",
    "sinon-chai": "2.8.0",
    "supertest": "1.1.0"
  },
  "dependencies": {
    "babel-cli": "6.3.17",
    "babel-core": "6.3.26",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015-node5": "1.1.1",
    "bluebird": "3.1.1",
    "kcors": "1.0.1",
    "koa": "1.1.2",
    "koa-bodyparser": "2.0.1",
    "koa-jwt": "1.1.1",
    "koa-route": "2.4.2"
