{
  "name": "koa-babel-6-boilerplate",
  "author": "Jordan Denison",
  "version": "0.0.1",
  "main": "src/server.js",
  "private": true,
  "scripts": {
    "cover": "node_modules/.bin/babel-node node_modules/.bin/isparta cover --report html --report lcov node_modules/.bin/_mocha",
    "lint": "eslint src --fix",
